{
  "development": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "mysql",
    "logging": false,
    "define": {
      "charset": "utf8",
      "dialectOptions": {
        "collate": "utf8_general_ci"
      },
      "pool": {
        "max": 5,
        "idle": 30000,
        "acquire": 60000
      },
      "freezeTableName": true,
      "underscored": true
    },
    "timezone": "-03:00"
  },
  "production": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "mysql",
    "logging": false,
    "define": {
      "charset": "utf8",
      "dialectOptions": {
        "collate": "utf8_general_ci"
      },
      "pool": {
        "max": 5,
        "idle": 30000,
        "acquire": 60000
      },
      "freezeTableName": true,
      "underscored": true
    },
    "timezone": "-03:00"
  }
}
